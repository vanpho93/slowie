{
  "rules": {
    "class-name": true,
    "indent": [
      true,
      "spaces"
    ],
    "one-line": [
      true,
      "check-open-brace",
      "check-whitespace"
    ],
    "no-unused-variable": true,
    "no-return-await": true,
    "no-consecutive-blank-lines": true,
    "no-duplicate-imports": true,
    "no-require-imports": true,
    "no-var-keyword": true,
    "no-unused-expression": true,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "trailing-comma": [
      true,
      {
        "singleline": "never",
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        }
      }
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ],
    "no-trailing-whitespace": true,
    "no-inferrable-types": true,
    "prefer-const": true,
    "comment-format": false,
    "no-duplicate-variable": true,
    "no-conditional-assignment": true,
    "no-console": [
      true,
      "error"
    ],
    "no-debugger": true,
    "no-empty": true,
    "no-eval": true,
    "restrict-plus-operands": true,
    "switch-default": true,
    "triple-equals": true,
    "use-isnan": true,
    "arrow-return-shorthand": true,
    "encoding": true,
    "import-spacing": true,
    "interface-name": [
      true,
      "always-prefix"
    ],
    "new-parens": true,
    "no-boolean-literal-compare": true,
    "no-unnecessary-callback-wrapper": true,
    "number-literal-format": true,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "strict-boolean-expressions": [
      true,
      "allow-null-union",
      "allow-undefined-union",
      "allow-boolean-or-undefined",
      "allow-string",
      "allow-mix"
    ]
  }
}
